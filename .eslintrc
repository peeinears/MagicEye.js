{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "vars-on-top": 0,
    "no-var": 0,
    "no-underscore-dangle": 0,
    "prefer-rest-params": 0,
    "guard-for-in": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-restricted-syntax": 0,
    "prefer-arrow-callback": 0,
    "object-shorthand": 0,
    "prefer-template": 0
  },
  "globabls": {
    "MagicEye": true
  }
}